{
  "openapi": "3.0.1",
  "info": {
    "title": "InterChoice API",
    "description": "Here you can find API documentation for our service",
    "license": {
      "name": "Our Team"
    },
    "version": "v1"
  },
  "paths": {
    "/Register": {
      "post": {
        "tags": [
          "Account"
        ],
        "parameters": [
          {
            "name": "FirstName",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "LastName",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Email",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "BirthDate",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Country",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "PasswordHash",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "password"
            }
          },
          {
            "name": "PasswordHashCheck",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "password"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Login": {
      "post": {
        "tags": [
          "Account"
        ],
        "parameters": [
          {
            "name": "Email",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "PasswordHash",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "password"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": { }
}